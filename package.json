{
  "name": "rpwhale-bot",
  "version": "1.0.0",
  "private": true,
  "description": "rpwhale.online donations and feature request github bot",
  "author": "Fran Guijarro <franleplant@gmail.com>",
  "license": "ISC",
  "homepage": "https://github.com/franleplant/rpwhale-bot",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "start": "probot run ./lib/index.js",
    "test": "yarn typecheck && yarn test:format && jest",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier \"**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "format": "yarn prettier --write",
    "test:format": "prettier --check"
  },
  "dependencies": {
    "@waxio/waxjs": "^0.0.15",
    "eosjs": "^22.0.0",
    "node-fetch": "^2.6.1",
    "probot": "^11.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.19",
    "@types/node-fetch": "^2.5.10",
    "husky": ">=6",
    "jest": "^26.6.3",
    "lint-staged": ">=10",
    "nock": "^13.0.5",
    "prettier": "^2.3.0",
    "smee-client": "^1.2.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">= 10.13.0"
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  }
}
